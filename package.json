{
  "name": "@origami/monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r test && pnpm -r build",
    "build:core": "pnpm --filter @llm/core build",
    "build:store": "pnpm --filter @llm/store build",
    "build:ui": "pnpm --filter @llm/ui build",
    "build:utils": "pnpm --filter @llm/utils build",
    "build:pc": "pnpm --filter @llm/pc-app build",
    "build:mobile": "pnpm --filter @llm/mobile-app build",
    "build:ui-less": "pnpm --filter @llm/ui-less build",
    "build:pc-less": "pnpm --filter @llm/pc-app-less build",
    "build:mobile-less": "pnpm --filter @llm/mobile-app-less build",
    "build:all": "pnpm -r build",
    "dev:pc": "pnpm --filter @llm/pc-app dev",
    "dev:mobile": "pnpm --filter @llm/mobile-app dev",
    "dev:pc-less": "pnpm --filter @llm/pc-app-less dev",
    "dev:mobile-less": "pnpm --filter @llm/mobile-app-less dev",
    "test": "pnpm -r test",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf node_modules **/*/node_modules"
  },
  "keywords": [
    "llm-chat-ui",
    "monorepo",
    "tailwindcss",
    "react",
    "chat",
    "vite",
    "zustand",
    "typescript"
  ],
  "author": "Daoxing.Huang <",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/less": "^3.0.8",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "less": "^4.5.1",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "rollup": "^4.52.4",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vitest": "^0.32.4",
    "zustand": "^4.5.7",
    "jsdom": "^27.2.0",
    "rimraf": "^5.0.10",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.js": "eslint --fix",
    "*.json": "prettier --write"
  },
  "dependencies": {
    "react-dom": "^18.3.1"
  }
}
